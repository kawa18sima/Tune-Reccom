<div class="whole">
  <div class="header">
  </div>

  <div class="theme">
    <ul class="theme__list">
      <% @themes.each do |theme| %>
        <li class="theme__item" id="theme_<%= theme.id %>"><%= theme.name %></li>
      <% end %>
    </ul>
    <div class="theme__new">
      <div class="theme__new_btn">+</div>
    </div>
  </div>

  <div class="contents">
    <div class="contents__left"></div>
    <div class="contents__right"></div>
  </div>
</div>
